{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build Debug",
            "command": "C:\\Program Files\\dotnet\\dotnet.exe",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/MMAService/MMAService.csproj",
                "-c",
                "Debug",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build Release",
            "command": "C:\\Program Files\\dotnet\\dotnet.exe",
            "type": "process",
